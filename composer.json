{
  "name": "alex-patterson-webdev/laminas-doctrine",
  "description": "A Doctrine integration module for Laminas Applications",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.0",
    "alex-patterson-webdev/doctrine-entity-repository": "dev-feature/0.2.0",
    "alex-patterson-webdev/laminas-doctrine-query-filter": "dev-feature/0.2.0",
    "alex-patterson-webdev/laminas-date-time": "dev-feature/0.1.0",
    "alex-patterson-webdev/laminas-factory": "^2",
    "alex-patterson-webdev/laminas-symfony-console": "dev-feature/0.1.0",
    "doctrine/doctrine-laminas-hydrator" : "^2",
    "doctrine/data-fixtures": "^1.4",
    "doctrine/orm": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasDoctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasDoctrine\\": "test/unit/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
